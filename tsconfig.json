{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist/server",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@grpc/client": ["server/grpc"],
      "@utils/*": ["server/utils/*"],
      "@proto*": ["server/grpc/proto/*"],
    }
  },
  "include": ["server/**/*"],
  "exclude":[
    "dist",
    "node_modules"
  ]
}
