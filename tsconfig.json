{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist/parser",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@db": ["parser/db"],
      "@config": ["parser/config.json"],
      "@api": ["parser/api"],
      "@utils": ["parser/utils"],
      "@tests*": ["parser/tests/*"],
      "@grpc/client": ["parser/grpc"],
      "@proto*": ["parser/grpc/proto/*"],
    }
  },
  "include": ["parser/**/*"],
  "exclude":[
    "dist",
    "node_modules"
  ]
}
